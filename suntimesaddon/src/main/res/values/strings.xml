<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">SuntimesAddon</string>
    <string name="app_url"><![CDATA[
        <a href="https://forrestguice.github.io/SuntimesWidget/">forrestguice.github.io/SuntimesWidget</a>
    ]]></string>    <!-- formatted link -->


    <string name="app_provider_version_missing"><![CDATA[<font color="#e65100"><b>Missing!</b></font>]]></string>
    <string name="missing_dependency"><![CDATA[
        This app requires<br />
        <b><u>%s</u></b> or greater!
    ]]></string>

    <string name="app_provider_version_denied"><![CDATA[<font color="#e65100"><b>Permission Denied!</b></font>]]></string>
    <string name="missing_permission"><![CDATA[
        Permission Denied!<br />
        <u>%s</u> must be reinstalled to work correctly.
        <br /><br />
        This app lacks permissions that are granted during installation. This happens when apps are installed in the wrong order - install Suntimes before installing add-ons.
    ]]></string>

    <!-- text size -->
    <string name="textSize_small">Small</string>
    <string name="textSize_normal">Normal</string>
    <string name="textSize_large">Large</string>
    <string name="textSize_xlarge">Extra Large</string>
    <string-array name="textSize_display">                      <!-- one-to-one with textSize_values -->
        <item>@string/textSize_small</item>
        <item>@string/textSize_normal</item>
        <item>@string/textSize_large</item>
        <item>@string/textSize_xlarge</item>
    </string-array>
    <string-array name="textSize_values" translatable="false">  <!-- one-to-one with textSize_display -->
        <item>SMALL</item>
        <item>NORMAL</item>
        <item>LARGE</item>
        <item>XLARGE</item>
    </string-array>

    <!-- time & date formats -->
    <string name="format_day">EEEE</string>                       <!-- Thursday -->
    <string name="format_year">yyyy</string>                      <!-- 2020 -->
    <string name="format_date">MMMM d</string>                    <!-- February 27 -->
    <string name="format_date_long">MMMM d, yyyy</string>         <!-- February 27, 2020 -->
    <string name="format_datetime_short"><xliff:g id="month">%1$s</xliff:g>, <xliff:g id="time">%2$s</xliff:g></string>           <!-- shortDate, time ... e.g. December 3, 11:00 am -->
    <string name="format_datetime_long"><xliff:g id="monthYear">%1$s</xliff:g>, <xliff:g id="time">%2$s</xliff:g></string>        <!-- longDate, time ... e.g. December 3, 2017, 11:00 am -->

    <string name="format_time12">h:mm a</string>                           <!-- 2:30 PM -->
    <string name="format_time12_withseconds">h:mm:ss a</string>            <!-- 2:30:15 PM -->
    <string name="format_time12_veryshort">h:mm</string>                 <!-- shortest 12hr time format; should omit am/pm -->
    <string name="format_time12_veryshort_withseconds">h:mm:ss</string>  <!-- shortest 12hr time format (w/ seconds); should omit am/pm -->
    <string name="format_time12_short"><xliff:g id="hoursMinutes">%1$s</xliff:g> <xliff:g id="suffix">%2$s</xliff:g></string>
    <string name="format_time24">HH:mm</string>                   <!-- 14:30 -->
    <string name="format_time24_withseconds">HH:mm:ss</string>  <!-- 24hr time format (w/ seconds) -->

    <string name="time_lmst">Local Sidereal Time</string>
    <string name="time_gmst">Greenwich Sidereal Time</string>
    <string name="time_utc">Coordinated Universal Time</string>
    <string name="time_localMean">Local Mean Time</string>
    <string name="time_apparent">Apparent Solar Time</string>

    <string name="timeFormatMode_system">System format</string>
    <string name="timeFormatMode_12hr">12-hour</string>
    <string name="timeFormatMode_24hr">24-hour</string>

    <string name="delta_format"><xliff:g id="value" example="3">%1$s</xliff:g><xliff:g id="units" example="m">%2$s</xliff:g></string>
    <string name="delta_years">y</string>    <!-- e.g. 1y -->
    <string name="delta_weeks">w</string>    <!-- e.g. 2w -->
    <string name="delta_days">d</string>     <!-- e.g. 1d -->
    <string name="delta_hours">h</string>    <!-- e.g. 2h -->
    <string name="delta_minutes">m</string>  <!-- e.g. 3m -->
    <string name="delta_seconds">s</string>  <!-- e.g. 4s -->
    <string name="delta_day_shorter">shorter</string>
    <string name="delta_day_longer">longer</string>

    <!-- position formats -->
    <string name="format_tag">[%1$s]</string>                     <!-- [tag] -->
    <string name="format_location">%s, %s</string>                <!-- lat, lon -->
    <string name="format_location_long">%s, %s %s</string>        <!-- lat, lon [alt] -->
    <string name="format_location_altitude">%1$s%2$s</string>     <!-- altitude, e.g. 1600m -->

    <string name="degrees_format"><xliff:g id="value" example="45">%1$s</xliff:g>°</string>  <!-- e.g. 45° -->
    <string name="elevation_format"><xliff:g id="degrees" example="45">%1$s</xliff:g><xliff:g id="altSymbol" example="∠">%2$s</xliff:g></string>  <!-- e.g. 45°∠ -->
    <string name="declination_format"><xliff:g id="degrees" example="45">%1$s</xliff:g><xliff:g id="decSymbol" example="δ">%2$s</xliff:g></string>  <!-- e.g. 5° δ -->
    <string name="rightascension_format"><xliff:g id="degrees" example="45">%1$s</xliff:g><xliff:g id="raSymbol" example="δ">%2$s</xliff:g></string>  <!-- e.g. 5° α -->
    <string name="direction_format"><xliff:g id="degrees" example="180°">%1$s</xliff:g>&#160;<xliff:g id="directions_short" example="S">%2$s</xliff:g></string>  <!-- e.g. 180°S -->
    <string name="distance_format"><xliff:g id="value" example="6">%1$s</xliff:g>&#160;<xliff:g id="units_short" example="ft">%2$s</xliff:g></string>  <!-- e.g. 6 ft (shadow length), 250,000 mi (distance to moon) -->

    <string name="widgetLabel_altitude_symbol">∠</string>
    <string name="widgetLabel_rightAscension_symbol">α</string>
    <string name="widgetLabel_declination_symbol">δ</string>

